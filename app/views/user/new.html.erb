<main>
  <h1>新規登録</h1>
  <div class="user-form">
    <%= form_tag("/user/create", {multipart: :true}) do %>
      <p>ユーザー名</p>
      <input type="text" name="name"></input>
      <p>メールアドレス</p>
      <input type="email" name="email"></input>
      <p>パスワード</p>
      <input type="password" name="password"></input>
      <input type="submit" value="登録">
    <% end %>
  </div>
</main>